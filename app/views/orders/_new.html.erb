<h1>Add New Order</h1>
<br>

<%= form_with scope: :order, url: orders_path, local: true do |form| %>

<p>
  <%= form.label :UserId %><br>
  <%= form.text_field :order_id%>
</p>

<p>
  <%= form.label :ProductId %><br>
  <%= form.text_field :product_id%>
</p>

<p>
  <%= form.label :Quantity %><br>
  <%= form.text_field :quantity %>
</p>

<p>
  <%= form.label :TotalPrice %><br>
  <%= form.text_field :total_price %>
</p>

<p>
  <%= form.submit %>
</p>

<% end %>

<%= link_to 'Back', orders_path %>
<br>
<br>
<%= link_to 'Back to dashboard', root_path %>
